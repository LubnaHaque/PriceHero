<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lubnasweety.pricehero">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".completed.starting">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".completed.login" />
        <activity android:name=".completed.register" />
        <activity android:name=".old.Details" />
        <activity android:name=".completed.Homepage" />
        <activity android:name=".Booking" />
        <activity
            android:name=".old.SideBar"
            android:label="PriceHero"
            android:theme="@style/AppTheme" />
        <activity android:name=".completed.ItemDetails" />
        <activity
            android:name=".completed.SellerHome"
            android:label="@string/title_activity_seller_home" />
        <activity android:name=".EditProfile" />
        <activity android:name=".completed.Selection" />
        <activity android:name=".old.GroceryActivity" />
        <activity android:name=".Buyer_Notification" />
        <activity android:name=".Cart"></activity>
    </application>

</manifest>